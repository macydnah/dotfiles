{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "workspace": {
    "library": [
      "$VIMRUNTIME",
      "$XDG_DATA_HOME/nvim",
      "$HOME/.local/share/nvim"
    ]
  },
  "runtime": {
    "version": "LuaJIT",
    "path": [
      "lua/?.lua",
      "lua/?/init.lua",
      "/usr/share/luajit-2.1/jit/?.lua",
      "/usr/share/lua/5.1/?.lua",
      "/usr/share/lua/5.1/?/init.lua"
    ]
  }
}
